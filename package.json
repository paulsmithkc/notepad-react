{
  "name": "@ranken/notepad_mongodb",
  "version": "1.0.0",
  "description": "Notepad example project w/ MongoDB",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config index.js",
    "nodemon": "nodemon -r dotenv/config index.js",
    "build": "webpack --mode production --env.production",
    "devserver": "webpack-dev-server --mode development --env.development --env.devServer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulsmithkc/base-node-curriculum.git"
  },
  "author": "Paul Smith",
  "license": "MIT",
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "engines": {
    "node": "12.16.3"
  },
  "env": {
    "DEBUG": {
      "description": "Which debug channels should be monitored?",
      "value": "app:*"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "config": "^3.3.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "joi-objectid": "^3.0.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "file-loader": "^6.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
